<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.bitproject.dao.ReportDao">

  <resultMap type="report" id="reportMap">
		<id column="repo_no" property="repoNo"/>
		<result column="mno" property="mno"/>
		<result column="target_no" property="targetNo"/>
		<result column="type" property="type"/>
		<result column="date" property="contents"/>
		<result column="contents" property="date"/>
		<result column="status" property="status"/>
  </resultMap>

  
  <insert id="insert" parameterType="Report">
    insert into report(writer,title,contents,address,party_fee,meeting_date,max_member,alcohol_type,alcohol_limit) 
    values(#{writer.mno},#{title},#{contents},#{address},#{partyFee},#{meetingDate},#{maxMember},#{alcoholType},#{alcoholLimit})
  </insert>
  

  </mapper>






